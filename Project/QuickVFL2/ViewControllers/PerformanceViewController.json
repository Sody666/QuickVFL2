{
    "labelResult":{
        ":className":"UILabel",
        ":viewData":{
            "numberOfLines":2
        }
    },
    "buttonsWrapper_":{
        "buttonVFL_":{
            ":className":"UIButton",
            ":viewData":{
                "text":"VFL",
                "textColor":"#000000",
                "backgroundColor":"#777777",
                "tapEvent":"onVFLButtonClicked",
            }
        },
        "buttonXIB_":{
            ":className":"UIButton",
            ":viewData":{
                "text":"XIB",
                "textColor":"#000000",
                "backgroundColor":"#777777",
                "tapEvent":"onXIBButtonClicked",
            }
        },
        "buttonMSY_":{
            ":className":"UIButton",
            ":viewData":{
                "text":"Masonry",
                "textColor":"#000000",
                "backgroundColor":"#777777",
                "tapEvent":"onMasonryButtonClicked",
            }
        },
        ":layout":"V:|-[buttonXIB_]-|; H:|-[buttonVFL_]-[buttonXIB_(buttonVFL_)]-[buttonMSY_(buttonVFL_)]-| {top, bottom};"
    },
    "viewPlaceHolder":{
        ":className":"UIView",
        ":viewData":{
            "backgroundColor":"#eeeeee"
        }
    },
    "_label_":{
        ":className":"UILabel",
        ":viewData":{
            "text":"It should be like below:"
        }
    },
    "cardDemo":{
        ":className":"TravelCardVFL",
        ":viewData":{
            "backgroundColor":"#eeeeee"
        }
    },
    ":scrollV":"scrollView",
    ":layout":"
        H:|-[labelResult]-|;
        V:|-[labelResult]-[viewPlaceHolder(cardDemo)]-[buttonsWrapper_]-[_label_]-[cardDemo]-| {left,right};
        H:[viewPlaceHolder(cardDemo)];
    "
}
