{
    "labelTitle":{
        ":viewData":{
            "numberOfLines":2,
            "className":"UILabel",
            "fontSize":18
        }
    },
    "imageViewLeft":{
        ":className":"UIImageView",
        ":heightEqual":"imageViewLeft@0.6@w"
    },
    "imageViewMiddle":"UIImageView",
    "imageViewRight":"UIImageView",
    "labelPostscript":{
        ":stayWhenCompressed":"H@100",
        ":stayWhenStretched":"H@100",
        ":viewData":{
            "className":"UILabel",
            "fontSize":12
        }
    },
    "buttonDislike_":{
        ":stayWhenCompressed":"H@101",
        ":stayWhenStretched":"H@101",
        ":viewData":{
            "className":"UIButton",
            "text":"x",
            "textColor":"#000000"
        }
    },
    ":layout":"
        H:|-[labelTitle]-|;
        H:|-[imageViewLeft]-[imageViewMiddle(imageViewLeft)]-[imageViewRight(imageViewLeft)]-| {top, bottom};
        H:|-[labelPostscript]-[buttonDislike_]-| {top, bottom};
        V:|[labelTitle]-5-[imageViewLeft]-5-[labelPostscript]|;
    "
}
